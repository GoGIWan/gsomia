<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="50dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        tools:ignore="UseCompoundDrawables">

        <ImageView
            android:id="@+id/sidebar"
            android:layout_width="50dp"
            android:layout_height="25dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/ic_menu"
            tools:ignore="ContentDescription" />


        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|center_horizontal"
            android:layout_marginStart="130dp"
            android:layout_marginLeft="130dp"
            android:layout_marginBottom="5dp"
            android:lineSpacingExtra="0sp"
            android:textColor="#000000"
            android:textSize="20sp"
            tools:text="SBD" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_marginTop="0dp"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="50dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_marginTop="0dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/charge"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/충전"
            android:src="@drawable/ic_battery_unknown" />

        <View

            android:layout_width="0.5dp"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="30dp"
            android:background="#000000" />

        <ImageView
            android:id="@+id/calender"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_alarm"
            tools:ignore="ContentDescription,RtlHardcoded" />

        <View

            android:layout_width="0.5dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:background="#000000" />


        <ImageView
            android:id="@+id/running"
            android:layout_width="30dp"
            android:layout_height="25dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ic_directions_run"
            tools:ignore="ContentDescription" />

        <View

            android:layout_width="0.5dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:background="#000000" />

        <ImageView
            android:id="@+id/bell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="15dp"
            android:contentDescription="@string/알림"
            android:src="@drawable/ic_local_dining" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:background="#000000" />


    <LinearLayout
        android:layout_width="1600dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:baselineAligned="false"
        android:orientation="horizontal">

        <ScrollView
            android:layout_width="1600dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            tools:ignore="InefficientWeight,UselessParent">

            <LinearLayout
                android:layout_width="1600dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/view"
                    android:layout_width="300dp"
                    android:layout_height="200dp"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="40dp"
                    android:layout_marginLeft="40dp"
                    android:contentDescription="@string/gage50"
                    android:src="@drawable/gage50" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="-175dp"
                    android:layout_marginLeft="-175dp"
                    android:layout_marginTop="100dp"
                    android:text="@string/_1_500"
                    android:textSize="20sp"
                    android:textStyle="bold" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="280dp"
                    android:layout_marginLeft="280dp"
                    android:layout_marginTop="-80dp"
                    android:contentDescription="@string/todo4"
                    android:src="@drawable/head" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-85dp"
                    android:layout_marginLeft="-85dp"
                    android:layout_marginTop="-32dp"
                    android:contentDescription="@string/todo5"
                    android:src="@drawable/healing_neck" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-84dp"
                    android:layout_marginLeft="-84dp"
                    android:layout_marginTop="-23dp"
                    android:contentDescription="@string/todo6"
                    android:src="@drawable/healing_neck1" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-120dp"
                    android:layout_marginLeft="-120dp"
                    android:layout_marginTop="15dp"
                    android:contentDescription="@string/todo7"
                    android:src="@drawable/dagerous_arm" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-35dp"
                    android:layout_marginLeft="-35dp"
                    android:layout_marginTop="3dp"
                    android:contentDescription="@string/recovery_chase"
                    android:src="@drawable/b_recovery_chase" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-13dp"
                    android:layout_marginLeft="-13dp"
                    android:layout_marginTop="10dp"
                    android:contentDescription="@string/todo9"
                    android:src="@drawable/dangerous_left_arm" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-139dp"
                    android:layout_marginLeft="-139dp"
                    android:layout_marginTop="61dp"
                    android:src="@drawable/healing_side"
                    tools:ignore="ContentDescription" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-29dp"
                    android:layout_marginLeft="-29dp"
                    android:layout_marginTop="80dp"
                    android:src="@drawable/b_recovery_adobeman"
                    android:contentDescription="@string/recovery_adobeman" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="-27dp"
                    android:layout_marginLeft="-27dp"
                    android:layout_marginTop="67dp"
                    android:src="@drawable/healing_left_side"
                    tools:ignore="ContentDescription" />

                <ImageView
                    android:id="@+id/view2"
                    android:layout_width="350dp"
                    android:layout_height="250dp"
                    tools:ignore="ContentDescription" />


            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:orientation="vertical">

        <View
            android:id="@+id/view7"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_gravity="center_vertical|center_horizontal"
            android:background="#D00000" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/button1"
                    android:layout_width="380dp"
                    android:layout_height="60dp"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/button_background">

                    <ImageView
                        android:id="@+id/symbol1"
                        android:layout_width="60dp"
                        android:layout_height="32dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="15dp"
                        android:src="@drawable/a_weightlifting"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/text"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center"
                        android:text="전완근"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <TextView
                        android:id="@+id/text1"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="50dp"
                        android:layout_marginLeft="50dp"
                        android:gravity="center"
                        android:text="@string/_50"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/심박수"
                    android:layout_width="380dp"
                    android:layout_height="60dp"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/button_background">

                    <ImageView
                        android:id="@+id/symbol2"
                        android:layout_width="60dp"
                        android:layout_height="32dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="15dp"

                        android:src="@drawable/heart"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/text3"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center"
                        android:text="심박 수"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <TextView
                        android:id="@+id/text4"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="50dp"
                        android:layout_marginLeft="50dp"
                        android:gravity="center"
                        android:text="@string/_50"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/button2"
                    android:layout_width="380dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/button_background">

                    <ImageView
                        android:id="@+id/symbol3"
                        android:layout_width="60dp"
                        android:layout_height="80dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="15dp"

                        android:src="@drawable/hiking"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/text5"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center"
                        android:text=""
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/text6"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="50dp"
                        android:layout_marginLeft="50dp"
                        android:gravity="center"
                        android:text=""
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/button3"
                    android:layout_width="378dp"
                    android:layout_height="54dp"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/button_background">

                    <ImageView
                        android:id="@+id/symbol4"
                        android:layout_width="60dp"
                        android:layout_height="32dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="15dp"
                        android:src="@drawable/a_weightlifting"
                        android:contentDescription="@string/weightlifting" />

                    <TextView
                        android:id="@+id/text7"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center"
                        android:text="전완근"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <TextView
                        android:id="@+id/text8"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="50dp"
                        android:layout_marginLeft="50dp"
                        android:gravity="center"
                        android:text="@string/_50"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/button4"
                    android:layout_width="380dp"
                    android:layout_height="60dp"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/button_background">

                    <ImageView
                        android:id="@+id/symbol6"
                        android:layout_width="60dp"
                        android:layout_height="32dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="15dp"
                        android:src="@drawable/a_weightlifting"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/text9"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="center"
                        android:text="전완근"
                        android:textStyle="bold"
                        tools:ignore="HardcodedText" />

                    <TextView
                        android:id="@+id/text10"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="50dp"
                        android:layout_marginLeft="50dp"
                        android:gravity="center"
                        android:text="@string/_50"
                        android:textStyle="bold" />

                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/teal_67"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="45dp"
            android:layout_height="35dp"
            android:layout_marginStart="60dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ic_home"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="45dp"
            android:layout_height="35dp"
            android:layout_marginStart="80dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ic_person"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="45dp"
            android:layout_height="35dp"
            android:layout_marginStart="80dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ic_settings"
            tools:ignore="ContentDescription" />
    </LinearLayout>


</LinearLayout>


